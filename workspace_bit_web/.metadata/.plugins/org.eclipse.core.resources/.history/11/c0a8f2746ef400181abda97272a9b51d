package com.bit.empapp.contoller;

import javax.annotation.Resource;

import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.RequestMapping;

import com.bit.empapp.service.EmpMapperCallService;

@Controller
public class EmpDeptController {
	
	@Resource(name="empService")
	EmpMapperCallService empService;
	
	
	@RequestMapping("result/empAllOut")
	public void  empAllList(Model model) {
	   model.addAttribute("empAll",	empService.callAllEmp());
	}
}
